        <Label x:Name="label_Copy1" Content="created for your everyday needs." HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top" Height="27"/>
        <Label x:Name="label_Copy2" Content="Made by John Thompson and Jared Nesbit" HorizontalAlignment="Left" Margin="10,123,0,0" VerticalAlignment="Top" Height="27"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="231" Margin="162,-13,0,-35" VerticalAlignment="Top" Width="248" Source="youdontsay-art.png"/>
        <Button x:Name="btnExit" Content="OK" HorizontalAlignment="Left" Margin="162,155,0,0" VerticalAlignment="Top" Width="75"/>


