<Window x:Class="CagedTunes.AboutForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CagedTunes"
        mc:Ignorable="d"
        Title="CagedTunes - About" Height="214.285" Width="415.966" ResizeMode="NoResize" Background="#FFFFFEF4">


    <Grid Margin="0,0,0,2">
        <Label x:Name="label" Content="CagedTunes:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="18.667"/>
